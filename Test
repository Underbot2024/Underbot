local allowedNames = {
  "name1",
  "name2",
  "name3",
}

if not table.find(allowedNames, name(), true) then
    --Made By VivoDibra
  return modules.game_textwindow.onGameEditText(0, 6279, 10000, name() ..", tu no tienes permitido usar estos scripts.", "Scripts Underbot", "")
end

modules.game_textmessage.displayGameMessage("Scripts Protegidos Cargados.")

setDefaultTab("Main")

-- Npc Talk
macro(5000," Npc Task", function()
  NPC.say("hi")
  schedule(2000, function()
    NPC.say("task")
    schedule(2000, function()
  NPC.say("custom")
  schedule(2000, function()
    NPC.say("long")
    schedule(2000, function()
    NPC.say("yes")
    schedule(2000, function()
      end)
    end)
   end)
  end)
 end)
end)
